package com.apitesting.test;

public class Test {
	int longestString(String str){

		String s = "";
		int longStr =1;
		for(int i=0;i<str.length();i++){
		for(int j=i+1;j<str.length();j++)
		{
		if(str.charAt(i)!=str.charAt(j)){
			s=s+str.charAt(i);
			System.out.println("print s:"+s);

		}
		else {
			break;
		}
		}
		}

		return longStr;

		}

}
